{
  "manifest_version": 2,
  "name": "Bootstrap Format",
  "version": "0.1",
  "browser_action": {
    "default_icon": "bootstrap-icon.png"
  },
  "content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "css": ["style.css"],
    "js": ["jquery.js","script.js"]
  }
]
}